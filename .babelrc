{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-alias",
          [
            { "src": "./src/components", "expose": "components" },
            { "src": "./assets", "expose": "assets" },
            { "src": "./src/variables.js", "expose": "variables" }
          ]
        ]
      ]
    }
  }
}
