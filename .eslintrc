{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react", "react-native"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "jest": false,
    "test": false,
    "it": false,
    "expect": false,
    "describe": false,
    "require": false,
    "global": false,
    "beforeEach": false,
    "shallow": false,
    "module": false,
    "afterEach": false,
    "testSnapshot": false
  },
  "rules": {
    "react/display-name": "off"
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.3.1"
    }
  }
}
